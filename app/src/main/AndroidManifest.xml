<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="edu.byui.cs246.prettyripped" >

    <application
        android:name="com.orm.SugarApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:icon,android:theme" >
        <activity
            android:name=".Aactivities.SessionsActivity"
            android:label="@string/title_activity_sessions"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Aactivities.SessionActivity"
            android:label="@string/title_activity_session"
            android:parentActivityName=".Aactivities.SessionsActivity"
            android:theme="@style/AppTheme.NoActionBar" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Aactivities.SessionsActivity" />
        </activity>
        <activity android:name=".Aactivities.TestSugarActivty" >
        </activity>

        <meta-data android:name="DATABASE" android:value="prettyrippedsugar08.db"/>
        <meta-data android:name="VERSION" android:value="2"/>
        <meta-data android:name="QUERY_LOG" android:value="true"/>
        <meta-data android:name="DOMAIN_PACKAGE_NAME" android:value="edu.byui.cs246.prettyripped"/>
    </application>

</manifest>
